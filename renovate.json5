{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:best-practices',
    'local>univention/dev/projects/ucs-base-image//renovate/preset#v0.20.0',
  ],
  packageRules: [
    {
      matchDatasources: [
        'docker',
      ],
      pinDigests: true,
      matchPackageNames: [
        '/.*/',
      ],
    },
    {
      matchDatasources: [
        'docker',
      ],
      pinDigests: false,
      matchPackageNames: [
        '/^.*:latest$/',
      ],
    },
  ],
  semanticCommits: 'enabled',
  semanticCommitType: 'fix',
  semanticCommitScope: 'deps',
}
